pipeline {
    agent any
     triggers {
        cron('H */4 * * 1-5')
    }
    stages {
stage('Parallel Stage') {
         parallel {
                stage('A') {
                      steps {
                           echo "On Branch A"
                      }
                }
                stage('B') {
                      steps {
                           echo "On Branch B"
                      }
                }
        }
}
    }
}
