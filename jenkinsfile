pipeline {
        agent any
stages {
     stage('Build') {
          steps {
            echo "hallow world"
          }
     }
     stage('Test') {
          steps { 
                 sh '''
                  pwd

                  echo "this is 2nd stage build"
                  '''

          }

     }
     stage('Deploy') {
        steps {
            sh '''
            pwd
            mkdir tata
            cd tata
            touch venk.txt
            chmod 777  venk.txt
            echo " i got this " > venk.txt
            ls -lrt
            '''
        }

     }

} 
}