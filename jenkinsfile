pipeline {
        agent none 
stages {
    stage('Build') {
        agent{
            label 'Slave_1'
        }
        steps {
            sh '''
              touch cj.txt
              chmod 777 cj.txt

              ls -lrt 
              '''
               }
        }
    stage('Test') {
        agent{
            label 'Slave_1'
        }
           
        
        steps {
        echo  "gta test gold fgeeo"

        }

        }

    }


}
